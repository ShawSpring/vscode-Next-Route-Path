{
  "name": "next-route-path",
  "displayName": ".",
  "description": "display nextjs route pathname corresponding to active file in status bar.",
  "version": "0.0.2",
  "main": "./dist/extension.js",
  "publisher": "ShawSpring",
  "engines": {
    "vscode": "^1.88.0",
    "node": ">=20.0.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "path",
    "route",
    "next",
    "status bar"
  ],
  "license": "MIT",
  "activationEvents": [
    "workspaceContains:next.config.*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "NextRoutePath.showPathname",
        "title": "Show Pathname",
        "category": "NextRoutePath"
      },
      {
        "command": "NextRoutePath.copyUrl",
        "title": "Copy URL",
        "category": "NextRoutePath"
      }
    ],
    "configuration": {
      "title": "NextRoutePath",
      "properties": {
        "NextRoutePath.baseUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "Specifies the base URL of your project"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "mocha"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.32",
    "@types/mocha": "^10.0.6",
    "@types/node": "~18.19.31",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "earl": "^1.2.1",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
